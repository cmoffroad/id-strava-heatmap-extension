{
  "manifest_version": 2,
  "name": "iD Strava Heatmap",
  "description": "\"iD Strava Heatmap\" is a Chrome extension that seamlessly integrates the popular Strava Activity Heatmap into the iD editor. Easily overlay the heatmap data onto your workspace and gain valuable insights into popular routes and activity patterns.",
  "version": "0.1",
  "permissions": [
    "cookies",
    "*://*.strava.com/*",
    "*://*.openstreetmap.org/*"
  ],
  "content_scripts": [
    {
      "matches": [ 
        "https://www.openstreetmap.org/id*"
      ],
      "js": [
        "scripts/browser-polyfill.min.js",
        "scripts/content.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "scripts/browser-polyfill.min.js",
      "scripts/background.js"
    ]
  },
  "web_accessible_resources": [
    "scripts/script.js"
  ],
  "homepage_url": "https://github.com/cmoffroad/id-strava-heatmap-extension",
  "icons": {
    
  }
}